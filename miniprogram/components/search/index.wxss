/* components/search/index.wxss */
.container{
  width:750rpx;
  height:100rpx;
  padding:0 20rpx;
  box-sizing: border-box;

  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;

  background-color: #2e3033;
}

.input{
  width:600rpx;
  height:60rpx;
  border-radius: 30rpx;
  background-color: #363b40;

  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.input input {
  flex: 1;
  padding: 0 20rpx;
  color: #eee;
}

.btn {
  color: #eee;
}

